Depth <- c("random", 0, 1, "random", 0, 1, "random", 0, 1)
Case <- c(rep("Easy", 3), rep("Medium", 3), rep("Hard", 3))
WinRate <- c(79, 100, 100, 59, 100, 100, 50, 100, 100)
df2 <- data.frame(Depth, Case, WinRate)
df2 %>%
group_by(Case) %>%
ggplot() +
geom_col(
aes(
x = factor(Case, levels=c("Easy", "Medium", "Hard")),
y = WinRate,
group = factor(Depth, levels=c("random", 0, 1)),
fill = factor(Depth, levels=c("random", 0, 1))
),
position = "dodge"
) +
labs(fill = "Depth",
x = "Difficulty")
