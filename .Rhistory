library(tidyverse)
library(ggrepel)
Depth <- c(1, 2, 3, 4, 1, 2, 3, 4)
Time <- c(0.341, 8.561, 214.574, 5267.744, 0.151, 2.954, 16.337, 243.913)
Method <- c(rep("original", 4), rep("alpha_beta", 4))
df <- data.frame(Depth, Time, Method)
ggplot(df, aes(x = Depth, y = Time, color = Method)) +
geom_point() +
geom_line() +
geom_text_repel(label = round(Time, 1), show.legend = FALSE) +
scale_color_manual(values = c("steelblue", "darkred")) +
labs(title="Time complexity of implementing alpha-beta pruning",
y = "Mean Time (s)") +
theme(plot.title = element_text(hjust = 0.5))
